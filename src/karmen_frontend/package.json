{
  "name": "karmen",
  "description": "A common interface for multiple 3D printers",
  "repository": {
    "type": "git",
    "url": "https://github.com/fragaria/karmen"
  },
  "license": "AGPL-3.0-only",
  "version": "0.0.0",
  "dependencies": {
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-router-dom": "^5.0.1",
    "react-scripts": "3.1.1"
  },
  "scripts": {
    "lint": "eslint src --ext jsx,js",
    "start": "BROWSER=none react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "build-env-file": "node ./scripts/build-env-file.js",
    "docker-start-dev": "npm rebuild node-sass && PORT=8080 npm start",
    "docker-start": "npm run build-env-file && npm run serve",
    "serve": "./node_modules/.bin/superstatic --debug 1 --port 8080 --host 0.0.0.0 --compression 1 build/"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "eslint": "^6.3.0",
    "node-sass": "^4.12.0",
    "superstatic": "^6.0.4"
  }
}
